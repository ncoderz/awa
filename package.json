{
  "name": "@zen/cli",
  "version": "1.0.0",
  "description": "TypeScript CLI tool for generating AI coding agent configuration files",
  "type": "module",
  "bin": {
    "zen": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --fix .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["cli", "template", "generator", "ai-agent", "typescript"],
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "chalk": "^5.3.0",
    "citty": "^0.1.6",
    "degit": "^2.8.4",
    "eta": "^3.5.0",
    "smol-toml": "^1.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/degit": "^2.8.6",
    "@types/node": "^22.10.1",
    "@vitest/coverage-v8": "^2.1.8",
    "fast-check": "^3.23.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
